Algoritmo "semnome"

Var
  AnoNas,AnoAtual,idade:inteiro


Inicio
   escreva("digite o ano do nascimento")
   leia(AnoNas)
   escreva("digite o ano atual")
   leia(AnoAtual)

   idade <- AnoAtual - AnoNas

   se(idade<18) entao
     escreva("nada a dizer")
   senao
     escreva("bem vindo a vida adulta")
   fimse

Fimalgoritmo
